CREATE DEFINER=`root`@`localhost` PROCEDURE `sp_consultar_nombre_cliente`(in_idCliente int(11))
begin
SET @id_persona = (select idPersona from clientes where idCliente = in_idCliente);
select concat( nombre ,' ', apellido) as NombreCompleto from personas where idPersona=@id_persona;
end ;;
