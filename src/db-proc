CREATE DEFINER=`root`@`localhost` PROCEDURE `sp_insert_prod_detalle`(in_legajo int(11))
select p.nombre as nombreProducto , pr.nombre as NombreProveedor  , p.descripcion , p.medida , n.cantidad , p.precioUnitario as PrecioUnitario, SUM(p.precioUnitario*n.cantidad) as Total 
 from detallenotapedidos n join productos p on n.codigoProducto = p.codigo 
							join proveedores pr on pr.idProveedor = p.idProveedor 
							join notapedidos ntp on ntp.idNotaPedido = n.idNotaPedido
                            join usuarios us on us.legajo = ntp.legajo
                            where n.idNotaPedido = in_legajo ;;
DELIMITER ;
