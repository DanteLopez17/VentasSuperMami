CREATE DEFINER=`root`@`localhost` PROCEDURE `sp_update_usuario`(in_idPersona int(11) ,in_idLegajo  int(11) , in_password varchar(250),in_nombre varchar(100), in_apellido varchar(100),in_direccion varchar(50) ,in_email varchar(100),in_telefono int ,in_activo int(11))
BEGIN


    UPDATE ventas.usuarios
    SET
    password=in_password,
		activo = in_activo

    WHERE legajo =in_idLegajo ;
    
      
    UPDATE ventas.personas
    SET
        nombre =  in_nombre,
        apellido = in_apellido ,
        direccion = in_direccion ,
        email =in_email ,
        telefono = in_telefono
    WHERE idPersona = in_idPersona ;
    
  

END ;;
DELIMITER ;
