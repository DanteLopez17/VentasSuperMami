CREATE DEFINER=`root`@`localhost` PROCEDURE `sp_update_producto`(in_marca varchar(50),
in_nombre  varchar(50),
in_stock int,
in_precioUnitario float,
in_medida float,
in_descripcion varchar(100),
in_activo int,
in_idProveedor int)
INSERT INTO `ventas`.`productos`
(
`marca`,
`nombre`,
`stock`,
`precioUnitario`,
`medida`,
`descripcion`,
`activo`,
`idProveedor`)
VALUES
(
in_marca,
in_nombre,
in_stock,
in_precioUnitario,
in_medida,
in_descripcion,
in_activo,
in_idProveedor) ;;
DELIMITER ;
