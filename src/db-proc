DELIMITER ;;
CREATE DEFINER=`root`@`localhost` PROCEDURE `sp_select_prod_detalle`(in_legajo int(11))
select distinct(n.idDetalle) ,p.nombre as nombreProducto , pr.nombre as NombreProveedor  , p.descripcion , p.medida , n.cantidad , p.precioUnitario as PrecioUnitario , sum(p.precioUnitario*n.cantidad) as Total
 from detallenotapedidos n join productos p on n.codigoProducto = p.codigo 
							join proveedores pr on pr.idProveedor = p.idProveedor 
							join notapedidos ntp on ntp.idNotaPedido = n.idNotaPedido
                            join usuarios us on us.legajo = ntp.legajo
                            where n.idNotaPedido = 1
                            group by n.idDetalle ;;
DELIMITER ;
